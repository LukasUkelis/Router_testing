{
  "modules":
  [
    {
      "moduleName":"ios",
      "targets":
      [
        {
          "target" : "System hostname",
          "returnFormat" : "string",
          "registerAddress" : "7",
          "numberOfReg" : "16",
          "sshCommand" : "ubus call system board | jsonfilter -e '@.hostname'" 
        },
        {
          "target" : "LAN MAC address",
          "returnFormat" : "string",
          "registerAddress" : "55",
          "numberOfReg" : "16",
          "sshCommand" : "ubus call mnfinfo get | jsonfilter -e '@.mnfinfo.mac'" 
        },
        {
          "target" : "Router name",
          "returnFormat" : "string",
          "registerAddress" : "71",
          "numberOfReg" : "16",
          "sshCommand" : "uci get system.@system[0].routername" 
        }
      ]
    },
    {
      "moduleName":"dual_sim",
      "targets":
      [
        {
          "target" : "Active SIM card",
          "returnFormat" : "int",
          "registerAddress" : "205",
          "numberOfReg" : "1",
          "sshCommand" : "ubus call sim get | jsonfilter -e '@.sim'" 
        }
      ]
    },
    {
      "moduleName":"gps",
      "targets":
      [
        {
          "target" : "GPS satellite count",
          "returnFormat" : "int",
          "registerAddress" : "181",
          "numberOfReg" : "2",
          "sshCommand" : "ubus call gpsd position | jsonfilter -e '@.satellites'" 
        },
        {
          "target" : "GPS speed",
          "returnFormat" : "int",
          "registerAddress" : "179",
          "numberOfReg" : "2",
          "sshCommand" : "ubus call gpsd position | jsonfilter -e '@.speed'" 
        },
        {
          "target" : "GPS accuracy",
          "returnFormat" : "float",
          "registerAddress" : "183",
          "numberOfReg" : "2",
          "sshCommand" : "ubus call gpsd position | jsonfilter -e '@.accuracy'" 
        }
      ]
    },
    {
      "moduleName":"mobile",
      "targets":
      [
        {
          "target" : "Mobile data sent today (SIM1)",
          "returnFormat" : "int",
          "registerAddress" : "185",
          "numberOfReg" : "2",
          "sshCommand" : "ubus  call mdcollect get '{\"period\":\"day\",\"sim\":1,\"mode m\":\"3-1\",\"current\":false}' | jsonfilter -e '@.rx'" 
        },
        {
          "target" : "Mobile data received today (SIM1)",
          "returnFormat" : "int",
          "registerAddress" : "187",
          "numberOfReg" : "2",
          "sshCommand" : "ubus call mdcollect get '{\"period\":\"day\",\"sim\":1,\"mode m\":\"3-1\",\"current\":false}'| jsonfilter -e '@.tx'" 
        },
        {
          "target" : "Mobile data received this week (SIM1)",
          "returnFormat" : "int",
          "registerAddress" : "189",
          "numberOfReg" : "2",
          "sshCommand" : "ubus call mdcollect get '{\"period\":\"week\",\"sim\":1,\"mode m\":\"3-1\",\"current\":false}'| jsonfilter -e '@.rx'" 
        },
        {
          "target" : "Mobile data sent this week (SIM1)",
          "returnFormat" : "int",
          "registerAddress" : "191",
          "numberOfReg" : "2",
          "sshCommand" : "ubus call mdcollect get '{\"period\":\"week\",\"sim\":1,\"mode m\":\"3-1\",\"current\":false}'| jsonfilter -e '@.tx'" 
        },
        {
          "target" : "Mobile data received this month (SIM1)",
          "returnFormat" : "int",
          "registerAddress" : "193",
          "numberOfReg" : "2",
          "sshCommand" : "ubus call mdcollect get '{\"period\":\"month\",\"sim\":1,\"mode m\":\"3-1\",\"current\":false}'| jsonfilter -e '@.rx'" 
        },
        {
          "target" : "Mobile data sent this month (SIM1)",
          "returnFormat" : "int",
          "registerAddress" : "195",
          "numberOfReg" : "2",
          "sshCommand" : "ubus call mdcollect get '{\"period\":\"month\",\"sim\":1,\"mode m\":\"3-1\",\"current\":false}'| jsonfilter -e '@.tx'" 
        }
      ]
    }
  ]
}